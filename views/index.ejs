<html>
<head>
<title>survey form 2</title>
<link rel="stylesheet" href="/static/style.css">
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script type ="text/javascript" src="/socket.io/socket.io.js"></script>
<script type="text/javascript">
	$(document).ready(function(){
		var socket = io.connect();
		$('#form').submit(function(event){
			event.preventDefault();
			$.post()
			socket.emit('submitting_form', {notice: "notice"});
		});
		socket.on('server_response', function(data){
			console.log('the server says:' + data.response);
		})
	});
</script>
</head>
<body>
	 <h3>Survey Form in Node</h3>
	 <form id="form" action='/index' method='emit'>
		  Name: <input type='text' name='name'><br>
		  Dojo Location: <select name="location">
		  		<option value="sb">San Bernadino</option>
				<option value="la">Los Angeles</option>
				<option value="mo">Monterey</option>
				<option value="se">Seattle</option>
				<option value="vz">Caracas</option>
		  </select><br>
		  Comment: <textarea name="comment" rows="8" cols="40"></textarea>
		  <input type='submit' value='Submit'>
	 </form>

</body>
</html>
